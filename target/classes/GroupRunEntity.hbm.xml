<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.springapp.hibernate.GroupRunEntity" table="group_run" schema="" catalog="Ghostrunner">
        <id name="groupRunID" column="group_run_id"/>
        <many-to-one name="runsByRunId" class="com.springapp.hibernate.RunsEntity">
            <column name="run_id" not-null="true"/>
        </many-to-one>
        <many-to-one name="groupsByGroupId" class="com.springapp.hibernate.GroupsEntity">
            <column name="group_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>